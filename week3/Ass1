//Desktop Details

package week3Ass1;

public class Desktop extends Computer{
	
	public void deskTopSize() {
		System.out.println("64Bit");
		
	}
	public static void main(String[]args) {
		
	
       Desktop desk =new Desktop();
       
       desk.computerModel();
       desk.deskTopSize();
}
	
}


